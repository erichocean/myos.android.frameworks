<?xml version="1.0" encoding="UTF-8"?>
<project name="android_rules" default="debug">
<target name="run">
    <exec executable="adb">
        <arg value="shell"/>
        <arg value="am"/>
        <arg value="start"/>
        <arg value="-a"/>
        <arg value="android.intent.action.MAIN"/>
        <arg value="-n"/>
        <arg value="com.examples.color_space/.ColorSpaceNA"/>
    </exec>
</target>
<target name="quit">
    <exec executable="adb">
        <arg value="shell"/>
        <arg value="am"/>
        <arg value="force-stop"/>
        <arg value="com.examples.color_space"/>
    </exec>
</target>
</project>

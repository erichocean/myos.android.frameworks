<?xml version="1.0" encoding="UTF-8"?>
<project name="android_rules" default="debug">
<target name="run">
    <exec executable="adb">
        <arg value="shell"/>
        <arg value="am"/>
        <arg value="start"/>
        <arg value="-a"/>
        <arg value="android.intent.action.MAIN"/>
        <arg value="-n"/>
        <arg value="com.example.SanAngeles/.DemoActivity"/>
    </exec>
</target>
</project>
